
def add(a, b):
    return a + b

def subtract(a, b):
    return a - b

def multiply(a, b):
    return a * b

def divide(a, b):
    return a / b


def power(a, b):
    return a ** b

def modulus(a, b):
    return a % b

def add_ten(a):
    return a + 10

def double(a):
    return a * 2


print("Hello, world!")
print(add(1, 2))
print(subtract(1, 2))
