print("=== Testing Function Call Resolution ===")

def add_ten(x):
    return x + 10


def double(x):
    return x * 2


print("1. Creating composed function...")
math_pipeline = add_ten | double
print("Composed function created and stored")

print("2. Testing data pipeline (works)...")
data_result = 5 | add_ten | double
print("Data pipeline result: 30")

print("3. Now attempting function call...")
result = math_pipeline(5)
print("Function call result:")
print(result)

print("Function call test complete!") 