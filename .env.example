# AWS Credentials (Optional)
AWS_ACCESS_KEY_ID=                      # Your AWS access key ID
AWS_SECRET_ACCESS_KEY=                  # Your AWS secret access key

# OpenAI Configuration (Required)
OPENAI_API_KEY=                         # Required: Your OpenAI API key
MODEL_NAME=gpt-4                        # Optional: LLM model to use (default: gpt-4)

# Logging Configuration (Optional)
LOG_LEVEL=INFO                          # Optional: Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_DIR=logs                            # Optional: Directory for log files (default: logs)
LOG_FORMAT=json                         # Optional: Log format - json or text (default: text)
LOG_MAX_BYTES=10000000                  # Optional: Max log file size before rotation in bytes (default: 10MB)
LOG_BACKUP_COUNT=5                      # Optional: Number of backup log files to keep (default: 5)
LOG_CONSOLE_OUTPUT=true                 # Optional: Enable console logging - true/false (default: true)
