# Arithmetic operations test - covers mathematical expressions
log("Starting arithmetic test")

# Basic arithmetic
private:a = 10
private:b = 5
private:sum = private:a + private:b
private:diff = private:a - private:b
private:product = private:a * private:b
private:quotient = private:a / private:b

print(f"Addition: {private:a} + {private:b} = {private:sum}")
print(f"Subtraction: {private:a} - {private:b} = {private:diff}")
print(f"Multiplication: {private:a} * {private:b} = {private:product}")
print(f"Division: {private:a} / {private:b} = {private:quotient}")

# Integer division and modulo
private:int_div = 17 // 5
private:remainder = 17 % 5
print(f"Integer division: 17 // 5 = {private:int_div}")
print(f"Modulo: 17 % 5 = {private:remainder}")

# Exponentiation
private:power = 2 ** 3
print(f"Exponentiation: 2 ** 3 = {private:power}")

# Expression precedence
private:complex = 2 + 3 * 4 - 1
print(f"Precedence: 2 + 3 * 4 - 1 = {private:complex}")

# Parentheses
private:grouped = (2 + 3) * (4 - 1)
print(f"Parentheses: (2 + 3) * (4 - 1) = {private:grouped}")

log("Arithmetic test completed") 