{
  "include": [
    "dana",
    "tests"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.venv",
    "**/.pytest_cache",
    "**/.mypy_cache",
    "**/.ruff_cache",
    "**/build",
    "**/*.egg-info",
    "examples",
    "docs",
    "tmp",
    "demos"
  ],
  "ignore": [
    "**/dana_grammar_embedded.py"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.12",
  "pythonPlatform": "Darwin",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.12",
      "pythonPlatform": "Darwin",
      "extraPaths": [
        "./dana",
        "./tests"
      ]
    }
  ]
} 
