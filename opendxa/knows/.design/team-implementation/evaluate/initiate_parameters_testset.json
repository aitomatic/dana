  "test_cases": [
    {
      "test_id": "TEST_BGA256_EMI_SHIELD_001",
      "description": "BGA 256-pin EMI shielding optimization",
      "input": {
        "product_id": "BGA_256PIN_17X17_RevA",
        "substrate_material": "FR4_Substrate",
        "coating_material": "EMIShield-Ag8000",
        "nozzle_type": "Needle_250um",
        "layout": {
          "type": "BGA", 
          "size_mm": [17.0, 17.0],
          "shield_area_mm": [15.5, 15.5],
          "dispensing_pattern": "perimeter_enclosure"
        },
        "target_specification": {
          "conductivity_ohm_per_sq": [0.15, 0.8],
          "shield_continuity": ">95%",
          "bead_height_mm": [0.8, 1.2],
          "target_yield": 0.95,
          "max_cycle_time_sec": 15
        }
      },
      "expected_output": {
        "optimized_parameters": {
          "vision": {
            "lighting_level": 95,
            "match_threshold": 0.90,
            "camera_gain": 20
          },
          "motion": {
            "z_height_mm": 0.20,
            "xy_speed_mmps": 4.5,
            "pattern_mode": "continuous_perimeter"
          },
          "dispensing": {
            "valve_open_time_ms": 190,
            "air_pressure_psi": 29.2,
            "pressure_delay_ms": 12,
            "purge_time_ms": 2500
          }
        },
        "quality_metrics": {
          "conductivity_ohm_per_sq": 0.28,
          "shield_continuity_percent": 98,
          "bead_height_mm": 0.98,
          "yield": 0.96,
          "cycle_time_sec": 12.2
        }
      }
    },
