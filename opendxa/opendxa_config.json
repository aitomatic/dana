{
    "llm": {
        "preferred_models": [
            { "name": "openai:vllm-local-model", "required_api_keys": [] },
            { "name": "openai:gpt-4o-mini", "required_api_keys": ["OPENAI_API_KEY"] },
            { "name": "openai:gpt-4o", "required_api_keys": ["OPENAI_API_KEY"] },
            { "name": "anthropic:claude-3-5-sonnet-20240620", "required_api_keys": ["ANTHROPIC_API_KEY"] },
            { "name": "groq:llama3-70b-8192", "required_api_keys": ["GROQ_API_KEY"] },
            { "name": "mistral:mistral-large-latest", "required_api_keys": ["MISTRAL_API_KEY"] },
            { "name": "google:gemini-1.5-pro-latest", "required_api_keys": ["GOOGLE_API_KEY"] },
            { "name": "deepseek:deepseek-chat", "required_api_keys": ["DEEPSEEK_API_KEY"] },
            { "name": "cohere:command-r-plus", "required_api_keys": ["COHERE_API_KEY"] },
            { "name": "azure:gpt-4o", "required_api_keys": ["AZURE_OPENAI_API_KEY", "AZURE_OPENAI_ENDPOINT"] }
        ],
        "temperature": 0.7,
        "max_tokens": 2000,
        "model_configs": {
            "default": { "temperature": 0.7, "max_tokens": 4096, "timeout": 60.0, "retries": 3 },
            "openai:vllm-local-model": { "temperature": 0.2, "max_tokens": 8000, "timeout": 120.0 }
        },
        "provider_configs": {
            "openai": { "base_url": "https://api.openai.com/v1", "api_key": "${OPENAI_API_KEY}" },
            "anthropic": { "base_url": "https://api.anthropic.com/v1", "api_key": "${ANTHROPIC_API_KEY}" },
            "groq": { "base_url": "https://api.groq.com/openai/v1", "api_key": "${GROQ_API_KEY}" },
            "mistral": { "base_url": "https://api.mistral.ai/v1", "api_key": "${MISTRAL_API_KEY}" },
            "deepseek": { "base_url": "https://api.deepseek.com/v1", "api_key": "${DEEPSEEK_API_KEY}" },
            "google": { "api_key": "${GOOGLE_API_KEY}" }, "cohere": { "api_key": "${COHERE_API_KEY}" },
            "azure": { "endpoint": "${AZURE_OPENAI_ENDPOINT}", "api_key": "${AZURE_OPENAI_API_KEY}" }
        }
    }
}
