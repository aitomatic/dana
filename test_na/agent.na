import methods

agent GeneralAgent:
    name: str = "GeneralAgent"
    description: str = "A general agent that can solve any problem."


def solve(general_agent: GeneralAgent, query: str) -> str:
    if methods.decide(query):
        workflow = methods.plan | methods.execute
        return workflow(query)
    else:
        return methods.answer(query)

general_agent = GeneralAgent()

print(general_agent.solve("What is the capital of France?"))