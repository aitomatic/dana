"""Semantic type coercion makes resource outputs structured."""


agent Neo


resource_hilbert_wiki = get_resource('rag', sources=["https://en.wikipedia.org/wiki/David_Hilbert"])


struct MathematicianProfile:
    name: str
    birth_year: int
    death_year: int
    nationality: str
    contributions: list[str]


hilbert_profile: MathematicianProfile = Neo.reason("Summarize this mathematician's life and contributions", resources=[resource_hilbert_wiki])
print(hilbert_profile)
