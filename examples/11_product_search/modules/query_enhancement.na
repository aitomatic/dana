# Query Enhancement Module
# Handles user query processing and enhancement using LLM reasoning

from prompts import get_enhancement_prompt

def enhance_query(query: str) -> dict:
    """Extract structured information from user query using LLM"""
    prompt = get_enhancement_prompt(query)
    enhanced: dict = reason(prompt)
    
    return {
        "original_query": query,
        "enhanced": enhanced
    }
